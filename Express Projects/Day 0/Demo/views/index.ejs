<%- include ./partials/header %>

<h1>Products</h1>
<form action="/" method="POST">
  <input name="prod" placeholder="input new product" />
  <button type="submit">submit</button>
</form>

<ul>
  <% for (let i = 0; i < products.length; i++) { %>
  <div>
    <li>
      <%= products[i] %>
      <button>delete</button>
    </li>
  </div>
  <% } %>
</ul>

